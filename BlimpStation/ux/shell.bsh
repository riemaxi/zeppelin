class Greeter{
    hello(){
        return "hello you";
    }

    hello(name){
        return "hello you " + name + " ... on behalf of BlimpStation/ux";
    }

    add(a,b){
        return a + b;
    }

};

greeter = new Greeter();

hello(){
    return greeter.hello();
};

hello(name){
    return greeter.hello(name);
};

add(a,b){
    return greeter.add(a,b);
}